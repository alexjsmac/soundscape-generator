# Copy this file to .env and fill in your AWS credentials for local development
# These match the Heroku environment variables

# Required AWS credentials (same as Heroku)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Optional: use AWS profile instead of above credentials
# If set, will use ~/.aws/credentials profile instead of environment variables
# AWS_PROFILE=personal

# Optional: override default values
# S3_BUCKET=soundscape-generator-photos
# SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/667582492015/AmazonRekogntionVideoAnalysis
# SNS_TOPIC_ARN=arn:aws:sns:us-east-1:667582492015:AmazonRekognitionVideoAnalysis
# IAM_ROLE_ARN=arn:aws:iam::667582492015:role/RekognitionRole
